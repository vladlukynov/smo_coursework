<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="TOP_CENTER" maxWidth="-Infinity" minWidth="-Infinity" prefWidth="400.0" spacing="50.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.polytech.smo.view.SettingsController">
   <children>
      <Label text="Установка настроек">
         <font>
            <Font name="System Bold" size="24.0" />
         </font>
      </Label>
      <VBox alignment="TOP_CENTER" maxWidth="-Infinity" minWidth="-Infinity" prefWidth="198.0" spacing="10.0">
         <children>
            <TextField fx:id="sourceDeviceCountLabel" promptText="sourceDevicesCount" />
            <TextField fx:id="bufferDeviceCountLabel" promptText="bufferDevicesCount" />
            <TextField fx:id="processingDeviceCountLabel" promptText="processingDevicesCount" />
            <TextField fx:id="lambdaLabel" promptText="lambda" />
            <HBox spacing="10.0">
               <children>
                  <TextField fx:id="aLabel" promptText="a" />
                  <TextField fx:id="bLabel" promptText="b" />
               </children>
            </HBox>
         </children>
      </VBox>
      <Button mnemonicParsing="false" onAction="#onApplyButtonClick" text="Установить" />
   </children>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</VBox>
